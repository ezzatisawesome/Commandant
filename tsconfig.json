{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/layouts/*": ["./src/layouts/*"],
			"@/components/*": ["./src/components/*"],
			"@/ui/*": ["./src/components/ui/*"],
			"@/services/*": ["./src/services/*"],
			"@/stores/*": ["./src/stores/*"],
			"@/utils/*": ["./src/utils/*"],
			"@/styles/*": ["./src/styles/*"],
		}
	}
}
